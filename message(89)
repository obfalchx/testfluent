		tbtn.MouseButton1Click:Connect(function()
			for _,v in scroll:GetChildren() do
				if v:IsA("TextButton") then
					ts:Create(v,TweenInfo.new(.3),{BackgroundTransparency=1}):Play()
					ts:Create(v.SelectedTab,TweenInfo.new(0),{Size=UDim2.new(0,3,0,0)}):Play()
					ts:Create(v.IDK,TweenInfo.new(.3),{ImageTransparency=.4}):Play()
					ts:Create(v.Title,TweenInfo.new(.3),{TextTransparency=.4}):Play()
				end
			end
			ts:Create(tbtn,TweenInfo.new(.3),{BackgroundTransparency=.8}):Play()
			ts:Create(sel,TweenInfo.new(.3),{Size=UDim2.new(0,3,0,15)}):Play()
			ts:Create(ico,TweenInfo.new(.3),{ImageTransparency=0}):Play()
			ts:Create(ttl,TweenInfo.new(.3),{TextTransparency=0}):Play()
			for _,v in pl:GetChildren() do
				if v.Name==tname.."_Page" then upl:JumpTo(v) end
			end
		end)
		
		if not first then
			first=true
			ts:Create(tbtn,TweenInfo.new(.3),{BackgroundTransparency=.8}):Play()
