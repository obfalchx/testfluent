		if i.KeyCode==Enum.KeyCode.Insert then gui.Enabled=not gui.Enabled end
	end)
	
	local resizing=false
	db.InputBegan:Connect(function(i)
		if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then resizing=true end
	end)
	uis.InputEnded:Connect(function(i)
		if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then resizing=false end
	end)
	uis.InputChanged:Connect(function(i)
		if resizing and (i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch) then
			main.Size=UDim2.new(0,math.clamp(i.Position.X-main.AbsolutePosition.X,524,math.huge),0,math.clamp(i.Position.Y-main.AbsolutePosition.Y,322,math.huge))
			page.Size=UDim2.new(0,math.clamp(i.Position.X-page.AbsolutePosition.X-8,350,math.huge),0,math.clamp(i.Position.Y-page.AbsolutePosition.Y-8,270,math.huge))
			tab.Size=UDim2.new(0,148,0,math.clamp(i.Position.Y-tab.AbsolutePosition.Y-8,270,math.huge))
		end
	end)
	
	function w.Alert(cfg,msg)
		if alert:FindFirstChild("Frame") then alert.Frame:Destroy() end
