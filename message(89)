				st.Color=_G.Primary
				st.Parent=tf
				
				local c=Instance.new("Frame")
				c.Parent=tb
				c.BackgroundColor3=_G.Primary
				c.Position=UDim2.new(0,3,.5,0)
				c.Size=UDim2.new(0,14,0,14)
				c.AnchorPoint=Vector2.new(0,.5)
				Instance.new("UICorner",c).CornerRadius=UDim.new(0,10)
				
				local function upd()
					if state then
						st.Thickness=0
						c:TweenPosition(UDim2.new(0,18,.5,0),"Out","Sine",.2,true)
						ts:Create(c,TweenInfo.new(.4),{BackgroundColor3=_G.Dark}):Play()
						ts:Create(tb,TweenInfo.new(.4),{BackgroundColor3=_G.Primary}):Play()
					else
						st.Thickness=1
						c:TweenPosition(UDim2.new(0,3,.5,0),"Out","Sine",.2,true)
						ts:Create(c,TweenInfo.new(.4),{BackgroundColor3=_G.Primary}):Play()
						ts:Create(tb,TweenInfo.new(.4),{BackgroundColor3=_G.Dark}):Play()
					end
					pcall(cb,state)
				end
				
				tb.MouseButton1Click:Connect(function()
					state=not state
					upd()
				end)
				
				if def then upd() end
			end,
			Dropdown=function(cfg,txt,items,def,cb)
				local open=false
				local f=Instance.new("Frame")
				f.Parent=pg
				f.BackgroundColor3=_G.Primary
				f.BackgroundTransparency=.8
				f.Size=UDim2.new(1,0,0,40)
				Instance.new("UICorner",f).CornerRadius=UDim.new(0,5)
				
				local l=Instance.new("TextLabel")
				l.Parent=f
				l.BackgroundTransparency=1
				l.Position=UDim2.new(0,15,0,5)
				l.Font=Enum.Font.GothamSemibold
				l.Text=txt
				l.TextColor3=Color3.new(1,1,1)
				l.TextSize=13
				l.TextXAlignment=Enum.TextXAlignment.Left
				
				local sb=Instance.new("TextButton")
				sb.Parent=f
				sb.BackgroundColor3=_G.Dark
				sb.BackgroundTransparency=.1
				sb.Position=UDim2.new(1,-5,0,5)
				sb.Size=UDim2.new(0,100,0,30)
				sb.AnchorPoint=Vector2.new(1,0)
				sb.Font=Enum.Font.GothamMedium
				sb.TextSize=9
				sb.Text="   "..(def or "Select Items")
				sb.TextColor3=Color3.new(1,1,1)
				sb.TextXAlignment=Enum.TextXAlignment.Left
				sb.ClipsDescendants=true
				Instance.new("UICorner",sb).CornerRadius=UDim.new(0,5)
				
				local df=Instance.new("Frame")
				df.Parent=f
				df.BackgroundColor3=_G.Dark
				df.ClipsDescendants=true
				df.Size=UDim2.new(1,-10,0,100)
				df.Position=UDim2.new(0,5,0,40)
				df.Visible=false
				Instance.new("UICorner",df).CornerRadius=UDim.new(0,5)
				
				local ds=Instance.new("ScrollingFrame")
				ds.Parent=df
				ds.Active=true
				ds.BackgroundTransparency=1
				ds.Position=UDim2.new(0,0,0,10)
				ds.Size=UDim2.new(1,0,0,80)
				ds.ScrollBarThickness=3
				ds.ZIndex=3
				Instance.new("UIPadding",ds).PaddingLeft=UDim.new(0,15)
				local dll=Instance.new("UIListLayout")
				dll.Parent=ds
				dll.Padding=UDim.new(0,1)
				
				local function addItem(name)
					local ib=Instance.new("TextButton")
					ib.Parent=ds
					ib.BackgroundColor3=_G.Primary
					ib.BackgroundTransparency=1
					ib.Size=UDim2.new(1,0,0,30)
					ib.Font=Enum.Font.GothamSemibold
					ib.Text=tostring(name)
					ib.TextColor3=Color3.new(1,1,1)
					ib.TextSize=11
					ib.TextTransparency=.5
					ib.TextXAlignment=Enum.TextXAlignment.Left
					ib.ZIndex=4
					Instance.new("UIPadding",ib).PaddingLeft=UDim.new(0,8)
					Instance.new("UICorner",ib).CornerRadius=UDim.new(0,5)
					
					local si=Instance.new("Frame")
					si.Parent=ib
					si.BackgroundColor3=_G.Primary
					si.BackgroundTransparency=1
					si.Size=UDim2.new(0,3,.4,0)
					si.Position=UDim2.new(0,-8,.5,0)
					si.AnchorPoint=Vector2.new(0,.5)
					si.ZIndex=4
					Instance.new("UICorner",si).CornerRadius=UDim.new(0,999)
					
					ib.MouseEnter:Connect(function()
						ts:Create(ib,TweenInfo.new(.3),{TextTransparency=0,BackgroundTransparency=.8}):Play()
						ts:Create(si,TweenInfo.new(.3),{BackgroundTransparency=0}):Play()
					end)
					ib.MouseLeave:Connect(function()
						ts:Create(ib,TweenInfo.new(.3),{TextTransparency=.5,BackgroundTransparency=1}):Play()
						ts:Create(si,TweenInfo.new(.3),{BackgroundTransparency=1}):Play()
					end)
					ib.MouseButton1Click:Connect(function()
						open=false
						sb.Text="   "..ib.Text
						cb(ib.Text)
						ts:Create(df,TweenInfo.new(.3),{Size=UDim2.new(1,-10,0,0)}):Play()
						ts:Create(f,TweenInfo.new(.3),{Size=UDim2.new(1,0,0,40)}):Play()
						df.Visible=false
					end)
				end
				
				for _,v in items do addItem(v) end
				ds.CanvasSize=UDim2.new(0,0,0,dll.AbsoluteContentSize.Y)
				
				sb.MouseButton1Click:Connect(function()
					open=not open
					if open then
						df.Visible=true
						ts:Create(df,TweenInfo.new(.3),{Size=UDim2.new(1,-10,0,100)}):Play()
						ts:Create(f,TweenInfo.new(.3),{Size=UDim2.new(1,0,0,145)}):Play()
					else
						ts:Create(df,TweenInfo.new(.3),{Size=UDim2.new(1,-10,0,0)}):Play()
						ts:Create(f,TweenInfo.new(.3),{Size=UDim2.new(1,0,0,40)}):Play()
					end
				end)
				
				return {
					Add=function(cfg,name) addItem(name) end,
					Clear=function()
						sb.Text="   Select Items"
						open=false
						df.Visible=false
						for _,v in ds:GetChildren() do if v:IsA("TextButton") then v:Destroy() end end
					end
				}
			end,
			Slider=function(cfg,txt,min,max,def,cb)
				local f=Instance.new("Frame")
				f.Parent=pg
				f.BackgroundTransparency=1
				f.Size=UDim2.new(1,0,0,45)
				
				local sf=Instance.new("Frame")
				sf.Parent=f
				sf.BackgroundColor3=_G.Primary
				sf.BackgroundTransparency=.8
				sf.Size=UDim2.new(1,0,0,45)
				Instance.new("UICorner",sf).CornerRadius=UDim.new(0,5)
				
				local l=Instance.new("TextLabel")
				l.Parent=sf
				l.BackgroundTransparency=1
				l.Position=UDim2.new(0,15,.5,0)
				l.AnchorPoint=Vector2.new(0,.5)
				l.Font=Enum.Font.GothamSemibold
				l.Text=txt
				l.TextColor3=Color3.new(1,1,1)
				l.TextSize=13
				l.TextXAlignment=Enum.TextXAlignment.Left
				
				local bar=Instance.new("Frame")
				bar.Parent=sf
				bar.BackgroundColor3=_G.Primary
				bar.BackgroundTransparency=.8
				bar.Size=UDim2.new(0,100,0,4)
				bar.Position=UDim2.new(1,-10,.5,10)
				bar.AnchorPoint=Vector2.new(1,.5)
				Instance.new("UICorner",bar).CornerRadius=UDim.new(0,5)
				
				local fill=Instance.new("Frame")
				fill.Parent=bar
				fill.BackgroundColor3=_G.Dark
				fill.Size=UDim2.new(def/max,0,0,4)
				Instance.new("UICorner",fill).CornerRadius=UDim.new(0,5)
				
				local circle=Instance.new("Frame")
				circle.Parent=fill
				circle.BackgroundColor3=_G.Dark
				circle.Position=UDim2.new(1,0,0,-5)
				circle.AnchorPoint=Vector2.new(.5,0)
				circle.Size=UDim2.new(0,13,0,13)
				Instance.new("UICorner",circle).CornerRadius=UDim.new(0,100)
				
				local tb=Instance.new("TextBox")
				tb.Parent=sf
				tb.BackgroundColor3=_G.Dark
				tb.BackgroundTransparency=.1
				tb.Font=Enum.Font.Code
				tb.Size=UDim2.new(0,35,0,15)
				tb.AnchorPoint=Vector2.new(1,.5)
				tb.Position=UDim2.new(1,-10,.5,-10)
				tb.TextColor3=Color3.new(1,1,1)
				tb.TextSize=9
				tb.Text=tostring(def)
				tb.ClearTextOnFocus=false
				tb.TextXAlignment=Enum.TextXAlignment.Center
				Instance.new("UICorner",tb).CornerRadius=UDim.new(0,3)
				
				local drag=false
				circle.InputBegan:Connect(function(i)
					if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then drag=true end
				end)
				bar.InputBegan:Connect(function(i)
					if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then drag=true end
				end)
				uis.InputEnded:Connect(function(i)
					if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then drag=false end
				end)
				uis.InputChanged:Connect(function(i)
					if drag and (i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch) then
						local val=math.floor(((max-min)/100)*fill.AbsoluteSize.X+min)
						pcall(cb,val)
						tb.Text=tostring(val)
						fill.Size=UDim2.new(0,math.clamp(i.Position.X-fill.AbsolutePosition.X,0,100),0,4)
						circle.Position=UDim2.new(0,math.clamp(i.Position.X-fill.AbsolutePosition.X-5,0,100),0,-5)
					end
				end)
				tb.FocusLost:Connect(function()
					local v=tonumber(tb.Text) or min
					if v>max then v=max end
					if v<min then v=min end
					tb.Text=tostring(v)
					fill.Size=UDim2.new(v/max,0,0,4)
					circle.Position=UDim2.new(1,0,0,-5)
					pcall(cb,v)
				end)
			end,
			Textbox=function(cfg,txt,cb)
				local f=Instance.new("Frame")
				f.Parent=pg
				f.BackgroundColor3=_G.Primary
				f.BackgroundTransparency=.8
				f.Size=UDim2.new(1,0,0,35)
				Instance.new("UICorner",f).CornerRadius=UDim.new(0,5)
				
				local l=Instance.new("TextLabel")
				l.Parent=f
				l.BackgroundTransparency=1
				l.Position=UDim2.new(0,15,.5,0)
				l.AnchorPoint=Vector2.new(0,.5)
				l.Font=Enum.Font.GothamSemibold
				l.Text=txt
				l.TextColor3=Color3.new(1,1,1)
				l.TextSize=13
				l.TextXAlignment=Enum.TextXAlignment.Left
				
				local tb=Instance.new("TextBox")
				tb.Parent=f
				tb.BackgroundColor3=_G.Dark
				tb.BackgroundTransparency=.1
				tb.Position=UDim2.new(1,-5,.5,0)
				tb.AnchorPoint=Vector2.new(1,.5)
				tb.Size=UDim2.new(0,80,0,25)
				tb.Font=Enum.Font.GothamSemibold
				tb.Text=""
				tb.TextColor3=Color3.fromRGB(225,225,225)
				tb.TextSize=11
				tb.ClipsDescendants=true
				Instance.new("UICorner",tb).CornerRadius=UDim.new(0,5)
				tb.FocusLost:Connect(function() cb(tb.Text) end)
			end,
			Label=function(cfg,txt)
				local l=Instance.new("TextLabel")
				l.Parent=pg
				l.BackgroundTransparency=1
				l.Size=UDim2.new(1,0,0,20)
				l.Font=Enum.Font.GothamSemibold
				l.TextColor3=Color3.fromRGB(225,225,225)
				l.TextSize=13
				l.Text=txt
				l.TextXAlignment=Enum.TextXAlignment.Left
				Instance.new("UIPadding",l).PaddingLeft=UDim.new(0,2)
				return {Set=function(cfg,t) l.Text=t end}
			end,
			Seperator=function(cfg,txt)
				local f=Instance.new("Frame")
				f.Parent=pg
				f.BackgroundTransparency=1
				f.Size=UDim2.new(1,0,0,36)
				
				local l=Instance.new("TextLabel")
				l.Parent=f
				l.BackgroundTransparency=1
				l.AnchorPoint=Vector2.new(.5,1)
				l.Position=UDim2.new(.5,0,0,30)
				l.Size=UDim2.new(1,0,0,36)
				l.Font=Enum.Font.GothamBold
				l.Text=txt
				l.TextColor3=Color3.new(1,1,1)
				l.TextSize=14
				
				local line=Instance.new("Frame")
				line.Parent=f
				line.BackgroundColor3=_G.Primary
				line.BorderSizePixel=0
				line.AnchorPoint=Vector2.new(.5,.5)
				line.Position=UDim2.new(.5,0,0,25)
				line.Size=UDim2.new(0,txs:GetTextSize(txt,14,l.Font,Vector2.new(math.huge,math.huge)).X*.7,0,3)
				Instance.new("UICorner",line).CornerRadius=UDim.new(0,math.huge)
			end,
			Line=function()
				local f=Instance.new("Frame")
				f.Parent=pg
				f.BackgroundTransparency=1
				f.Size=UDim2.new(1,0,0,20)
				
				local line=Instance.new("Frame")
				line.Parent=f
				line.BorderSizePixel=0
				line.Position=UDim2.new(0,0,0,10)
				line.Size=UDim2.new(1,0,0,1)
				
				local g=Instance.new("UIGradient")
				g.Color=ColorSequence.new({
					ColorSequenceKeypoint.new(0,_G.Dark),
					ColorSequenceKeypoint.new(.4,_G.Primary),
					ColorSequenceKeypoint.new(.6,_G.Primary),
					ColorSequenceKeypoint.new(1,_G.Dark)
				})
				g.Parent=line
			end
		}
	end}
end

return w