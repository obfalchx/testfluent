		t2.Text=msg
		t2.TextColor3=Color3.fromRGB(200,200,200)
		t2.TextSize=12
		t2.TextXAlignment=Enum.TextXAlignment.Left
		
		f:TweenPosition(UDim2.new(1,-195,0,0),"Out","Quad",.4,true)
		wait(2)
		f:TweenPosition(UDim2.new(1,0,0,0),"Out","Quad",.5,true)
		wait(.6)
		f:Destroy()
	end
	
