			ts:Create(ico,TweenInfo.new(.3),{ImageTransparency=0}):Play()
			ts:Create(ttl,TweenInfo.new(.3),{TextTransparency=0}):Play()
			upl:JumpToIndex(1)
		end
		
		rs.Stepped:Connect(function()
			pcall(function()
				pg.CanvasSize=UDim2.new(0,0,0,pll.AbsoluteContentSize.Y)
				scroll.CanvasSize=UDim2.new(0,0,0,ll.AbsoluteContentSize.Y)
			end)
		end)
		
		return {
			Button=function(cfg,txt,cb)
				local f=Instance.new("Frame")
				f.Parent=pg
