			end,
			Toggle=function(cfg,txt,def,desc,cb)
				local state=def or false
				local f=Instance.new("TextButton")
				f.Parent=pg
				f.BackgroundColor3=_G.Primary
				f.BackgroundTransparency=.8
				f.Size=UDim2.new(1,0,0,46)
				f.AutoButtonColor=false
