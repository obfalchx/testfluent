local cg=game:GetService("CoreGui")
local uis=game:GetService("UserInputService")
local ts=game:GetService("TweenService")
local txs=game:GetService("TextService")
local rs=game:GetService("RunService")

if cg:FindFirstChild("RELZHUB") and cg:FindFirstChild("ScreenGui") then
	cg.RELZHUB:Destroy()
	cg.ScreenGui:Destroy()
end

local themes={
	Red={Color3.fromRGB(255,30,50),Color3.fromRGB(90,10,20)},
	Cyan={Color3.fromRGB(40,230,255),Color3.fromRGB(10,80,115)},
	Blue={Color3.fromRGB(40,155,255),Color3.fromRGB(10,80,115)},
	DarkBlue={Color3.fromRGB(50,30,255),Color3.fromRGB(20,10,90)},
	Green={Color3.fromRGB(70,255,205),Color3.fromRGB(20,90,90)},
	LightGreen={Color3.fromRGB(205,255,205),Color3.fromRGB(70,90,70)},
	Purple={Color3.fromRGB(205,125,255),Color3.fromRGB(60,20,95)},
	Zinc={Color3.fromRGB(30,30,30),Color3.fromRGB(10,10,10)}
}

local t=themes[_G.Theme]
if t then _G.Primary,_G.Dark=t[1],t[2]
else _G.Primary,_G.Dark=Color3.fromRGB(110,110,120),Color3.fromRGB(20,20,30) end

local btn=Instance.new("ScreenGui")
btn.Parent=cg
btn.ZIndexBehavior=Enum.ZIndexBehavior.Sibling

local img=Instance.new("ImageButton")
img.Parent=btn
img.Position=UDim2.new(0,10,0,10)
img.Size=UDim2.new(0,50,0,50)
img.Draggable=true
img.BackgroundColor3=_G.Dark
img.ImageColor3=_G.Primary
img.ImageTransparency=.1
img.BackgroundTransparency=.1
img.Image="rbxassetid://13940080072"

local us=Instance.new("UIStroke")
us.Color=_G.Primary
us.Parent=img

local uc=Instance.new("UICorner")
uc.CornerRadius=UDim.new(0,5)
uc.Parent=img

img.MouseButton1Click:Connect(function()
	local r=cg:FindFirstChild("RELZHUB")
	if r then r.Enabled=not r.Enabled end
end)

repeat wait(1) until game:IsLoaded()

local function drag(e,f)
	local dr,sp,fp,li
	e.InputBegan:Connect(function(i)
		if i.UserInputType==Enum.UserInputType.MouseButton1 or i.UserInputType==Enum.UserInputType.Touch then
			dr=true
			sp=i.Position
			fp=f.Position
			i.Changed:Connect(function()
				if i.UserInputState==Enum.UserInputState.End then dr=false end
			end)
		end
	end)
	e.InputChanged:Connect(function(i)
		if i.UserInputType==Enum.UserInputType.MouseMovement or i.UserInputType==Enum.UserInputType.Touch then li=i end
	end)
	uis.InputChanged:Connect(function(i)
		if i==li and dr then
			local d=i.Position-sp
			ts:Create(f,TweenInfo.new(.15),{Position=UDim2.new(fp.X.Scale,fp.X.Offset+d.X,fp.Y.Scale,fp.Y.Offset+d.Y)}):Play()
		end
	end)
end

local w={}

function w.Window(cfg,name)
	local first=false
	
	local alert=Instance.new("ScreenGui")
	alert.Name="AlertFrame"
	alert.Parent=cg
	alert.ZIndexBehavior=Enum.ZIndexBehavior.Global
	
	local gui=Instance.new("ScreenGui")
	gui.Name="RELZHUB"
	gui.Parent=cg
	gui.ZIndexBehavior=Enum.ZIndexBehavior.Global
	
	local main=Instance.new("Frame")
	main.Name="Main"
	main.Parent=gui
	main.ClipsDescendants=true
	main.AnchorPoint=Vector2.new(.5,.5)
	main.BackgroundColor3=_G.Dark
	main.BackgroundTransparency=.1
	main.Position=UDim2.new(.5,0,.5,0)
