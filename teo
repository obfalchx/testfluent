					btnLabel.TextSize = 13

					local clickBtn = Instance.new("TextButton")
					clickBtn.Name = "TextButton"
					clickBtn.Parent = btnFrame
					clickBtn.BackgroundColor3 = _G.Primary
					clickBtn.BackgroundTransparency = 0
					clickBtn.AnchorPoint = Vector2.new(1, .5)
					clickBtn.Position = UDim2.new(1, -10, .5, 0)
					clickBtn.Size = UDim2.new(0, 22, 0, 22)
					clickBtn.Font = Enum.Font.GothamSemibold
					clickBtn.Text = ""
					clickBtn.TextXAlignment = Enum.TextXAlignment.Left
					clickBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
					clickBtn.TextSize = 13

					local clickCorner = Instance.new("UICorner")
					clickCorner.CornerRadius = UDim.new(0, 4)
					clickCorner.Parent = clickBtn

					local btnIcon = Instance.new("ImageLabel")
					btnIcon.Name = "ImageLabel"
					btnIcon.Parent = clickBtn
					btnIcon.BackgroundColor3 = _G.Primary
					btnIcon.BackgroundTransparency = 1
					btnIcon.AnchorPoint = Vector2.new(.5, .5)
					btnIcon.Position = UDim2.new(.5, 0, .5, 0)
					btnIcon.Size = UDim2.new(0, 15, 0, 15)
					btnIcon.Image = "rbxassetid://10723375250"
					btnIcon.ImageTransparency = .2
					btnIcon.ImageColor3 = Color3.fromRGB(245, 245, 245)

					clickBtn.MouseButton1Click:Connect(function()
						callback()
					end)
				end,

				Toggle = function(self, text, desc, default, callback)
					local state = default or false
					_G.TrueColor = _G.Primary

					local toggleBtn = Instance.new("TextButton")
					toggleBtn.Name = "Button"
					toggleBtn.Parent = page
					toggleBtn.BackgroundColor3 = _G.Primary
					toggleBtn.BackgroundTransparency = .8
					toggleBtn.Size = UDim2.new(1, 0, 0, 46)
					toggleBtn.AutoButtonColor = false
					toggleBtn.Font = Enum.Font.SourceSans
					toggleBtn.Text = ""
					toggleBtn.TextColor3 = Color3.fromRGB(0, 0, 0)
					toggleBtn.TextSize = 11

					local toggleCorner = Instance.new("UICorner")
					toggleCorner.CornerRadius = UDim.new(0, 5)
					toggleCorner.Parent = toggleBtn

					local toggleLabel = Instance.new("TextLabel")
					toggleLabel.Parent = toggleBtn
					toggleLabel.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
					toggleLabel.BackgroundTransparency = 1
					toggleLabel.Size = UDim2.new(1, 0, 0, 35)
					toggleLabel.Font = Enum.Font.GothamSemibold
					toggleLabel.Text = text
					toggleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
					toggleLabel.TextSize = 13
					toggleLabel.TextXAlignment = Enum.TextXAlignment.Left
					toggleLabel.AnchorPoint = Vector2.new(0, .5)

					local descLabel = Instance.new("TextLabel")
					descLabel.Parent = toggleLabel
					descLabel.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
					descLabel.BackgroundTransparency = 1
					descLabel.Position = UDim2.new(0, 0, 0, 22)
					descLabel.Size = UDim2.new(0, 280, 0, 16)
					descLabel.Font = Enum.Font.Gotham
					descLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
					descLabel.TextSize = 10
					descLabel.TextXAlignment = Enum.TextXAlignment.Left

					if desc then
						descLabel.Text = desc
						toggleLabel.Position = UDim2.new(0, 15, .5, -5)
						descLabel.Position = UDim2.new(0, 0, 0, 22)
					else
						toggleLabel.Position = UDim2.new(0, 15, .5, 0)
						descLabel.Visible = false
					end

					local toggleFrame = Instance.new("Frame")
					toggleFrame.Name = "ToggleFrame"
					toggleFrame.Parent = toggleBtn
					toggleFrame.BackgroundColor3 = _G.Dark
					toggleFrame.BackgroundTransparency = 1
					toggleFrame.Position = UDim2.new(1, -10, .5, 0)
					toggleFrame.Size = UDim2.new(0, 35, 0, 20)
					toggleFrame.AnchorPoint = Vector2.new(1, .5)

					local frameCorner = Instance.new("UICorner")
					frameCorner.CornerRadius = UDim.new(0, 10)
					frameCorner.Parent = toggleFrame

					local toggleInner = Instance.new("TextButton")
					toggleInner.Name = "ToggleImage"
					toggleInner.Parent = toggleFrame
					toggleInner.BackgroundColor3 = _G.Dark
					toggleInner.BackgroundTransparency = 0
					toggleInner.Position = UDim2.new(0, 0, 0, 0)
					toggleInner.AnchorPoint = Vector2.new(0, 0)
					toggleInner.Size = UDim2.new(1, 0, 1, 0)
					toggleInner.Text = ""
					toggleInner.AutoButtonColor = false

					local innerStroke = Instance.new("UIStroke")
					innerStroke.Color = _G.Primary
					innerStroke.Thickness = 1
					innerStroke.Transparency = 0
					innerStroke.Parent = toggleFrame

					local innerCorner = Instance.new("UICorner")
					innerCorner.CornerRadius = UDim.new(0, 10)
					innerCorner.Parent = toggleInner

					local circle = Instance.new("Frame")
					circle.Name = "Circle"
					circle.Parent = toggleInner
					circle.BackgroundColor3 = _G.Primary
					circle.BackgroundTransparency = 0
					circle.Position = UDim2.new(0, 3, .5, 0)
					circle.Size = UDim2.new(0, 14, 0, 14)
					circle.AnchorPoint = Vector2.new(0, .5)

					local circleCorner = Instance.new("UICorner")
					circleCorner.CornerRadius = UDim.new(0, 10)
					circleCorner.Parent = circle

					toggleInner.MouseButton1Click:Connect(function()
						if state then
							state = false
							innerStroke.Thickness = 1
							circle:TweenPosition(UDim2.new(0, 3, .5, 0), "Out", "Sine", .2, true)
							ts:Create(circle, TweenInfo.new(.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = _G.Primary}):Play()
							ts:Create(toggleInner, TweenInfo.new(.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = _G.Dark}):Play()
						else
							state = true
							innerStroke.Thickness = 0
							circle:TweenPosition(UDim2.new(0, 18, .5, 0), "Out", "Sine", .2, true)
							ts:Create(circle, TweenInfo.new(.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = _G.Dark}):Play()
							ts:Create(toggleInner, TweenInfo.new(.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = _G.Primary}):Play()
						end
						pcall(callback, state)
					end)

					if default == true then
						innerStroke.Thickness = 0
						circle:TweenPosition(UDim2.new(0, 18, .5, 0), "Out", "Sine", .4, true)
						ts:Create(circle, TweenInfo.new(.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = _G.Dark}):Play()
						ts:Create(toggleInner, TweenInfo.new(.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundColor3 = _G.Primary}):Play()
						pcall(callback, true)
					end
				end,

				Dropdown = function(self, text, options, default, callback)
					local open = false

					local dropFrame = Instance.new("Frame")
					dropFrame.Name = "Dropdown"
					dropFrame.Parent = page
					dropFrame.BackgroundColor3 = _G.Primary
					dropFrame.BackgroundTransparency = .8
					dropFrame.ClipsDescendants = false
					dropFrame.Size = UDim2.new(1, 0, 0, 40)

					local dropCorner = Instance.new("UICorner")
					dropCorner.CornerRadius = UDim.new(0, 5)
					dropCorner.Parent = dropFrame

					local dropTitle = Instance.new("TextLabel")
					dropTitle.Name = "DropTitle"
					dropTitle.Parent = dropFrame
					dropTitle.BackgroundColor3 = _G.Primary
					dropTitle.BackgroundTransparency = 1
					dropTitle.Size = UDim2.new(1, 0, 0, 30)
					dropTitle.Font = Enum.Font.GothamSemibold
					dropTitle.Text = text
					dropTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
					dropTitle.TextSize = 13
					dropTitle.TextXAlignment = Enum.TextXAlignment.Left
					dropTitle.Position = UDim2.new(0, 15, 0, 5)
					dropTitle.AnchorPoint = Vector2.new(0, 0)

					local selectBtn = Instance.new("TextButton")
					selectBtn.Name = "SelectItems"
					selectBtn.Parent = dropFrame
					selectBtn.BackgroundColor3 = _G.Dark
					selectBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
					selectBtn.BackgroundTransparency = .1
					selectBtn.Position = UDim2.new(1, -5, 0, 5)
					selectBtn.Size = UDim2.new(0, 100, 0, 30)
					selectBtn.AnchorPoint = Vector2.new(1, 0)
					selectBtn.Font = Enum.Font.GothamMedium
					selectBtn.TextSize = 9
					selectBtn.ZIndex = 1
					selectBtn.Text = default and "   " .. default or "   Select Items"
					selectBtn.ClipsDescendants = true
					selectBtn.TextXAlignment = Enum.TextXAlignment.Left

					local selectCorner = Instance.new("UICorner")
					selectCorner.Parent = selectBtn
					selectCorner.CornerRadius = UDim.new(0, 5)

					local scrollFrame = Instance.new("Frame")
					scrollFrame.Name = "DropdownFrameScroll"
					scrollFrame.Parent = dropFrame
					scrollFrame.BackgroundColor3 = _G.Dark
					scrollFrame.BackgroundTransparency = 0
					scrollFrame.ClipsDescendants = true
					scrollFrame.Size = UDim2.new(1, -10, 0, 100)
					scrollFrame.Position = UDim2.new(0, 5, 0, 40)
					scrollFrame.Visible = false
					scrollFrame.AnchorPoint = Vector2.new(0, 0)

					local scrollCorner = Instance.new("UICorner")
					scrollCorner.Parent = scrollFrame
					scrollCorner.CornerRadius = UDim.new(0, 5)

					local dropScroll = Instance.new("ScrollingFrame")
					dropScroll.Name = "DropScroll"
					dropScroll.Parent = scrollFrame
					dropScroll.ScrollingDirection = Enum.ScrollingDirection.Y
					dropScroll.Active = true
					dropScroll.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					dropScroll.BackgroundTransparency = 1
					dropScroll.BorderSizePixel = 0
					dropScroll.Position = UDim2.new(0, 0, 0, 10)
					dropScroll.Size = UDim2.new(1, 0, 0, 80)
					dropScroll.AnchorPoint = Vector2.new(0, 0)
					dropScroll.ClipsDescendants = true
					dropScroll.ScrollBarThickness = 3
					dropScroll.ZIndex = 3

					local scrollPad = Instance.new("UIPadding")
					scrollPad.PaddingLeft = UDim.new(0, 10)
					scrollPad.PaddingRight = UDim.new(0, 10)
					scrollPad.Parent = dropScroll
					scrollPad.Name = "PaddingDrop"

					local scrollLayout = Instance.new("UIListLayout")
					scrollLayout.Parent = dropScroll
					scrollLayout.SortOrder = Enum.SortOrder.LayoutOrder
					scrollLayout.Padding = UDim.new(0, 1)

					local scrollPad2 = Instance.new("UIPadding")
					scrollPad2.Parent = dropScroll
					scrollPad2.PaddingLeft = UDim.new(0, 5)

					for _, opt in pairs(options) do
						local item = Instance.new("TextButton")
						item.Name = "Item"
						item.Parent = dropScroll
						item.BackgroundColor3 = _G.Primary
						item.BackgroundTransparency = 1
						item.Size = UDim2.new(1, 0, 0, 30)
						item.Font = Enum.Font.GothamSemibold
						item.Text = tostring(opt)
						item.TextColor3 = Color3.fromRGB(255, 255, 255)
						item.TextSize = 11
						item.TextTransparency = .5
						item.TextXAlignment = Enum.TextXAlignment.Left
						item.ZIndex = 4

						local itemPad = Instance.new("UIPadding")
						itemPad.Parent = item
						itemPad.PaddingLeft = UDim.new(0, 8)

						local itemCorner = Instance.new("UICorner")
						itemCorner.Parent = item
						itemCorner.CornerRadius = UDim.new(0, 5)

						local selected = Instance.new("Frame")
						selected.Name = "SelectedItems"
						selected.Parent = item
						selected.BackgroundColor3 = _G.Primary
						selected.BackgroundTransparency = 1
						selected.Size = UDim2.new(0, 3, .4, 0)
						selected.Position = UDim2.new(0, -8, .5, 0)
						selected.AnchorPoint = Vector2.new(0, .5)
						selected.ZIndex = 4

						local selCorner = Instance.new("UICorner")
						selCorner.Parent = selected
						selCorner.CornerRadius = UDim.new(0, 999)

						item.MouseEnter:Connect(function()
							ts:Create(item, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency = 0, BackgroundTransparency = .8}):Play()
							ts:Create(selected, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
						end)

						item.MouseLeave:Connect(function()
							ts:Create(item, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency = .5, BackgroundTransparency = 1}):Play()
							ts:Create(selected, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
						end)

						item.MouseButton1Click:Connect(function()
							open = false
							selectBtn.ClipsDescendants = true
							ts:Create(scrollFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, -10, 0, 0), Visible = false}):Play()
							callback(item.Text)
							selectBtn.Text = "   " .. item.Text
							ts:Create(dropFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, 0, 0, 40)}):Play()
						end)
					end

					dropScroll.CanvasSize = UDim2.new(0, 0, 0, scrollLayout.AbsoluteContentSize.Y)

					selectBtn.MouseButton1Click:Connect(function()
						if open then
							open = false
							ts:Create(scrollFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, -10, 0, 0), Visible = false}):Play()
							ts:Create(dropFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, 0, 0, 40)}):Play()
						else
							open = true
							ts:Create(scrollFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, -10, 0, 100), Visible = true}):Play()
							ts:Create(dropFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, 0, 0, 145)}):Play()
						end
					end)

					return {
						Add = function(self, value)
							local item = Instance.new("TextButton")
							item.Name = "Item"
							item.Parent = dropScroll
							item.BackgroundColor3 = _G.Primary
							item.BackgroundTransparency = 1
							item.Size = UDim2.new(1, 0, 0, 30)
							item.Font = Enum.Font.GothamSemibold
							item.Text = tostring(value)
							item.TextColor3 = Color3.fromRGB(255, 255, 255)
							item.TextSize = 11
							item.TextTransparency = .5
							item.TextXAlignment = Enum.TextXAlignment.Left
							item.ZIndex = 4

							local itemPad = Instance.new("UIPadding")
							itemPad.Parent = item
							itemPad.PaddingLeft = UDim.new(0, 8)

							local itemCorner = Instance.new("UICorner")
							itemCorner.Parent = item
							itemCorner.CornerRadius = UDim.new(0, 5)

							local selected = Instance.new("Frame")
							selected.Name = "SelectedItems"
							selected.Parent = item
							selected.BackgroundColor3 = _G.Primary
							selected.BackgroundTransparency = 1
							selected.Size = UDim2.new(0, 3, .4, 0)
							selected.Position = UDim2.new(0, -8, .5, 0)
							selected.AnchorPoint = Vector2.new(0, .5)
							selected.ZIndex = 4

							local selCorner = Instance.new("UICorner")
							selCorner.Parent = selected
							selCorner.CornerRadius = UDim.new(0, 999)

							item.MouseEnter:Connect(function()
								ts:Create(item, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency = 0, BackgroundTransparency = .8}):Play()
								ts:Create(selected, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
							end)

							item.MouseLeave:Connect(function()
								ts:Create(item, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {TextTransparency = .5, BackgroundTransparency = 1}):Play()
								ts:Create(selected, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
							end)

							item.MouseButton1Click:Connect(function()
								open = false
								callback(item.Text)
								selectBtn.Text = "   " .. item.Text
								ts:Create(scrollFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, -10, 0, 0), Visible = false}):Play()
								ts:Create(dropFrame, TweenInfo.new(.3, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {Size = UDim2.new(1, 0, 0, 40)}):Play()
							end)
						end,

						Clear = function(self)
							selectBtn.Text = "   Select Items"
							open = false
							scrollFrame.Visible = false
							for _, v in pairs(dropScroll:GetChildren()) do
								if v:IsA("TextButton") then v:Destroy() end
							end
						end
					}
				end,

				Slider = function(self, text, min, max, default, callback)
					local sliderFrame = Instance.new("Frame")
					sliderFrame.Name = "Slider"
					sliderFrame.Parent = page
					sliderFrame.BackgroundColor3 = _G.Primary
					sliderFrame.BackgroundTransparency = 1
					sliderFrame.Size = UDim2.new(1, 0, 0, 45)

					local sliderInner = Instance.new("Frame")
					sliderInner.Name = "sliderr"
					sliderInner.Parent = sliderFrame
					sliderInner.BackgroundColor3 = _G.Primary
					sliderInner.BackgroundTransparency = .8
					sliderInner.Position = UDim2.new(0, 0, 0, 0)
					sliderInner.Size = UDim2.new(1, 0, 0, 45)

					local innerCorner = Instance.new("UICorner")
					innerCorner.CornerRadius = UDim.new(0, 5)
					innerCorner.Parent = sliderInner

					local sliderLabel = Instance.new("TextLabel")
					sliderLabel.Parent = sliderInner
					sliderLabel.BackgroundColor3 = Color3.fromRGB(150, 150, 150)
					sliderLabel.BackgroundTransparency = 1
					sliderLabel.Position = UDim2.new(0, 15, .5, 0)
					sliderLabel.Size = UDim2.new(1, 0, 0, 30)
					sliderLabel.Font = Enum.Font.GothamSemibold
					sliderLabel.Text = text
					sliderLabel.AnchorPoint = Vector2.new(0, .5)
					sliderLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
					sliderLabel.TextSize = 13
					sliderLabel.TextXAlignment = Enum.TextXAlignment.Left

					local bar = Instance.new("Frame")
					bar.Name = "bar"
					bar.Parent = sliderInner
					bar.BackgroundColor3 = _G.Primary
					bar.Size = UDim2.new(0, 100, 0, 4)
					bar.Position = UDim2.new(1, -10, .5, 10)
					bar.BackgroundTransparency = .8
					bar.AnchorPoint = Vector2.new(1, .5)

					local barFill = Instance.new("Frame")
					barFill.Name = "bar1"
					barFill.Parent = bar
					barFill.BackgroundColor3 = _G.Dark
					barFill.BackgroundTransparency = 0
					barFill.Size = UDim2.new(default / max, 0, 0, 4)

					local fillCorner = Instance.new("UICorner")
					fillCorner.CornerRadius = UDim.new(0, 5)
					fillCorner.Parent = barFill

					local barCorner = Instance.new("UICorner")
					barCorner.CornerRadius = UDim.new(0, 5)
					barCorner.Parent = bar

					local circle = Instance.new("Frame")
					circle.Name = "circlebar"
					circle.Parent = barFill
					circle.BackgroundColor3 = _G.Dark
					circle.Position = UDim2.new(1, 0, 0, -5)
					circle.AnchorPoint = Vector2.new(.5, 0)
					circle.Size = UDim2.new(0, 13, 0, 13)

					local circleCorner = Instance.new("UICorner")
					circleCorner.CornerRadius = UDim.new(0, 100)
					circleCorner.Parent = circle

					local valueBox = Instance.new("TextBox")
					valueBox.Parent = sliderInner
					valueBox.BackgroundColor3 = _G.Dark
					valueBox.BackgroundTransparency = .1
					valueBox.Font = Enum.Font.Code
					valueBox.Size = UDim2.new(0, 35, 0, 15)
					valueBox.AnchorPoint = Vector2.new(1, .5)
					valueBox.Position = UDim2.new(1, -10, .5, -10)
					valueBox.TextColor3 = Color3.fromRGB(255, 255, 255)
					valueBox.TextSize = 9
					valueBox.Text = default
					valueBox.TextTransparency = .1
					valueBox.ClearTextOnFocus = false
					valueBox.TextXAlignment = Enum.TextXAlignment.Center

					local boxCorner = Instance.new("UICorner")
					boxCorner.CornerRadius = UDim.new(0, 3)
					boxCorner.Parent = valueBox

					local Value = default or 0
					pcall(callback, Value)

					local sliding = false

					circle.InputBegan:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							sliding = true
						end
					end)

					bar.InputBegan:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							sliding = true
						end
					end)

					uis.InputEnded:Connect(function(input)
						if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
							sliding = false
						end
					end)

					uis.InputChanged:Connect(function(input)
						if sliding and (input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch) then
							Value = math.floor(((tonumber(max) - tonumber(min)) / 100) * barFill.AbsoluteSize.X + tonumber(min)) or 0
							pcall(callback, Value)
							valueBox.Text = Value
							barFill.Size = UDim2.new(0, math.clamp(input.Position.X - barFill.AbsolutePosition.X, 0, 100), 0, 4)
							circle.Position = UDim2.new(0, math.clamp((input.Position.X - barFill.AbsolutePosition.X) - 5, 0, 100), 0, -5)
						end
					end)

					valueBox.FocusLost:Connect(function()
						if max < tonumber(valueBox.Text) then valueBox.Text = max end
						barFill.Size = UDim2.new((valueBox.Text or 0) / max, 0, 0, 4)
						circle.Position = UDim2.new(1, 0, 0, -5)
						valueBox.Text = tostring(valueBox.Text and math.floor((valueBox.Text / max) * (max - min) + min))
						pcall(callback, valueBox.Text)
					end)
				end,

				Textbox = function(self, text, placeholder, callback)
					local textFrame = Instance.new("Frame")
					textFrame.Name = "Textbox"
					textFrame.Parent = page
					textFrame.BackgroundColor3 = _G.Primary
					textFrame.BackgroundTransparency = .8
					textFrame.Size = UDim2.new(1, 0, 0, 35)

					local textCorner = Instance.new("UICorner")
					textCorner.CornerRadius = UDim.new(0, 5)
					textCorner.Parent = textFrame

					local textLabel = Instance.new("TextLabel")
					textLabel.Name = "TextboxLabel"
					textLabel.Parent = textFrame
					textLabel.BackgroundColor3 = _G.Primary
					textLabel.BackgroundTransparency = 1
					textLabel.Position = UDim2.new(0, 15, .5, 0)
					textLabel.Text = text
					textLabel.Size = UDim2.new(1, 0, 0, 35)
					textLabel.Font = Enum.Font.GothamSemibold
					textLabel.AnchorPoint = Vector2.new(0, .5)
					textLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
					textLabel.TextSize = 13
					textLabel.TextXAlignment = Enum.TextXAlignment.Left

					local textBox = Instance.new("TextBox")
					textBox.Name = "RealTextbox"
					textBox.Parent = textFrame
					textBox.BackgroundColor3 = _G.Dark
					textBox.BackgroundTransparency = .1
					textBox.Position = UDim2.new(1, -5, .5, 0)
					textBox.AnchorPoint = Vector2.new(1, .5)
					textBox.Size = UDim2.new(0, 80, 0, 25)
					textBox.Font = Enum.Font.GothamSemibold
					textBox.Text = ""
					textBox.TextColor3 = Color3.fromRGB(225, 225, 225)
					textBox.TextSize = 11
					textBox.ClipsDescendants = true

					local boxCorner = Instance.new("UICorner")
					boxCorner.CornerRadius = UDim.new(0, 5)
					boxCorner.Parent = textBox

					textBox.FocusLost:Connect(function()
						callback(textBox.Text)
					end)
				end,

				Label = function(self, text)
					local label = Instance.new("TextLabel")
					label.Name = "Label"
					label.Parent = page
					label.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					label.BackgroundTransparency = 1
					label.Size = UDim2.new(1, 0, 0, 20)
					label.Font = Enum.Font.GothamSemibold
					label.TextColor3 = Color3.fromRGB(225, 225, 225)
					label.TextSize = 13
					label.Text = text
					label.TextXAlignment = Enum.TextXAlignment.Left

					local labelPad = Instance.new("UIPadding")
					labelPad.PaddingLeft = UDim.new(0, 2)
					labelPad.Parent = label

					return {
						Set = function(self, newText)
							label.Text = newText
						end
					}
				end,

				Seperator = function(self, text)
					local sepFrame = Instance.new("Frame")
					sepFrame.Name = "Seperator"
					sepFrame.Parent = page
					sepFrame.BackgroundColor3 = _G.Primary
					sepFrame.BackgroundTransparency = 1
					sepFrame.Size = UDim2.new(1, 0, 0, 36)

					local sepLabel = Instance.new("TextLabel")
					sepLabel.Name = "Sep2"
					sepLabel.Parent = sepFrame
					sepLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					sepLabel.BackgroundTransparency = 1
					sepLabel.AnchorPoint = Vector2.new(.5, 1)
					sepLabel.Position = UDim2.new(.5, 0, 0, 30)
					sepLabel.Size = UDim2.new(1, 0, 0, 36)
					sepLabel.Font = Enum.Font.GothamBold
					sepLabel.Text = text
					sepLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
					sepLabel.TextSize = 14

					local sepLine = Instance.new("Frame")
					sepLine.Name = "Sep3"
					sepLine.Parent = sepFrame
					sepLine.BackgroundColor3 = _G.Primary
					sepLine.BackgroundTransparency = 0
					sepLine.BorderSizePixel = 0
					sepLine.AnchorPoint = Vector2.new(.5, .5)
					sepLine.Position = UDim2.new(.5, 0, 0, 25)
					sepLine.Size = UDim2.new(0, 39, 0, 3)

					local textSize = game:GetService("TextService"):GetTextSize(sepLabel.Text, sepLabel.TextSize, sepLabel.Font, Vector2.new(math.huge, math.huge))
					sepLine.Size = UDim2.new(0, textSize.X * .7, 0, 3)

					local lineCorner = Instance.new("UICorner")
					lineCorner.CornerRadius = UDim.new(0, math.huge)
					lineCorner.Parent = sepLine
				end,

				Line = function(self)
					local lineFrame = Instance.new("Frame")
					lineFrame.Name = "Linee"
					lineFrame.Parent = page
					lineFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
					lineFrame.BackgroundTransparency = 1
					lineFrame.Position = UDim2.new(0, 0, .119999997, 0)
					lineFrame.Size = UDim2.new(1, 0, 0, 20)

					local line = Instance.new("Frame")
					line.Name = "Line"
					line.Parent = lineFrame
					line.BackgroundColor3 = Color3.new(125, 125, 125)
					line.BorderSizePixel = 0
					line.Position = UDim2.new(0, 0, 0, 10)
					line.Size = UDim2.new(1, 0, 0, 1)

					local gradient = Instance.new("UIGradient")
					gradient.Color = ColorSequence.new({
						ColorSequenceKeypoint.new(0, _G.Dark),
						ColorSequenceKeypoint.new(.4, _G.Primary),
						ColorSequenceKeypoint.new(.5, _G.Primary),
						ColorSequenceKeypoint.new(.6, _G.Primary),
						ColorSequenceKeypoint.new(1, _G.Dark)
					})
					gradient.Parent = line
				end
			}
		end
	}
end

return lib