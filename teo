local cg = game:GetService("CoreGui")
if cg:FindFirstChild("Xeter") and cg:FindFirstChild("ScreenGui") then
	cg.Xeter:Destroy()
	cg.ScreenGui:Destroy()
end

local themes = {
	Red = {Color3.fromRGB(255, 30, 50), Color3.fromRGB(90, 10, 20)},
	Cyan = {Color3.fromRGB(40, 230, 255), Color3.fromRGB(10, 80, 115)},
	Blue = {Color3.fromRGB(40, 155, 255), Color3.fromRGB(10, 80, 115)},
	DarkBlue = {Color3.fromRGB(50, 30, 255), Color3.fromRGB(20, 10, 90)},
	Green = {Color3.fromRGB(70, 255, 205), Color3.fromRGB(20, 90, 90)},
	LightGreen = {Color3.fromRGB(205, 255, 205), Color3.fromRGB(70, 90, 70)},
	Purple = {Color3.fromRGB(205, 125, 255), Color3.fromRGB(60, 20, 95)},
	Zinc = {Color3.fromRGB(30, 30, 30), Color3.fromRGB(10, 10, 10)}
}

local theme = themes[_G.Theme] or {Color3.fromRGB(110, 110, 120), Color3.fromRGB(20, 20, 30)}
_G.Primary = theme[1]
_G.Dark = theme[2]

local lib = {}
repeat wait(1) until game:IsLoaded()

local uis = game:GetService("UserInputService")
local ts = game:GetService("TweenService")

local function drag(frame, target)
	local dragging, dragStart, startPos

	local function update(input)
		local delta = input.Position - dragStart
		ts:Create(target, TweenInfo.new(.15), {
			Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		}):Play()
	end

	frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = target.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then dragging = false end
			end)
		end
	end)

	frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragging then update(input) end
		end
	end)

	uis.InputChanged:Connect(function(input)
		if dragging then update(input) end
	end)
end

