				end)
			end)

			function lib.Alert(title, text)
				if cg:FindFirstChild("AlertFrame") then
					local af = cg.AlertFrame
					if af:FindFirstChild("Frame") then af.Frame:Destroy() end
				end

				local alertFrame = Instance.new("Frame")
				alertFrame.Name = "Frame"
