				alertText.TextSize = 12
				alertText.TextXAlignment = Enum.TextXAlignment.Left

				local alertCorner = Instance.new("UICorner")
				alertCorner.CornerRadius = UDim.new(0, 5)
				alertCorner.Parent = alertFrame

				alertFrame:TweenPosition(UDim2.new(1, -195, 0, 0), "Out", "Quad", .4, true)
				wait(2)
				alertFrame:TweenPosition(UDim2.new(1, 0, 0, 0), "Out", "Quad", .5, true)
				wait(.6)
				alertFrame:Destroy()
			end

			return {
				Button = function(self, text, callback)
					local btnFrame = Instance.new("Frame")
					btnFrame.Name = "Button"
					btnFrame.Parent = page
