				alertText.BackgroundTransparency = 1
				alertText.Position = UDim2.new(0, 55, 0, 33)
				alertText.Size = UDim2.new(0, 10, 0, 10)
				alertText.Font = Enum.Font.GothamSemibold
				alertText.TextTransparency = .3
				alertText.Text = text
